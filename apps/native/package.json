{
	"name": "native",
	"version": "1.0.0",
	"private": true,
	"main": "index.js",
	"scripts": {
		"dev": "expo start --clear",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"prebuild": "expo prebuild",
		"build:dev": "eas build --profile development --non-interactive",
		"build:dev:ios": "eas build --profile development --platform ios --non-interactive",
		"build:dev:android": "eas build --profile development --platform android --non-interactive",
		"build:dev:local": "expo prebuild && eas build --profile development --local --non-interactive",
		"build:dev:ios:local": "expo prebuild && eas build --profile development --platform ios --local --non-interactive --output ./build/dev.ipa && ios-deploy -b ./build/dev.ipa",
		"build:dev:android:local": "expo prebuild && eas build --profile development --platform android --local --non-interactive",
		"build:preview": "eas build --profile preview --non-interactive",
		"build:preview:ios": "eas build --profile preview --platform ios --non-interactive",
		"build:preview:android": "eas build --profile preview --platform android --non-interactive",
		"build:preview:local": "expo prebuild && eas build --profile preview --local --non-interactive",
		"build:preview:ios:local": "expo prebuild && eas build --profile preview --platform ios --local --non-interactive --output ./build/preview.ipa && ios-deploy -b ./build/preview.ipa",
		"build:preview:android:local": "expo prebuild && eas build --profile preview --platform android --local --non-interactive",
		"build:prod": "eas build --profile production --non-interactive",
		"build:prod:ios": "eas build --profile production --platform ios --non-interactive",
		"build:prod:android": "eas build --profile production --platform android --non-interactive",
		"build:prod:local": "expo prebuild && eas build --profile production --local --non-interactive",
		"build:prod:ios:local": "expo prebuild && eas build --profile production --platform ios --local --non-interactive --output ./build/prod.ipa && ios-deploy -b ./build/prod.ipa",
		"build:prod:android:local": "expo prebuild && eas build --profile production --platform android --local --non-interactive"
	},
	"dependencies": {
		"@expo/vector-icons": "^15.0.2",
		"@keyed/db": "workspace:*",
		"@keyed/engine": "workspace:*",
		"@react-navigation/bottom-tabs": "^7.3.10",
		"@react-navigation/drawer": "^7.3.9",
		"@react-navigation/native": "^7.1.6",
		"@shopify/react-native-skia": "2.2.12",
		"@siteed/react-native-essentia": "^0.3.1",
		"@tanstack/react-form": "^1.0.5",
		"expo": "^54.0.0",
		"expo-constants": "~18.0.11",
		"expo-crypto": "~15.0.8",
		"expo-dev-client": "~6.0.20",
		"expo-font": "~14.0.10",
		"expo-linking": "~8.0.10",
		"expo-router": "~6.0.17",
		"expo-secure-store": "~15.0.8",
		"expo-splash-screen": "~31.0.12",
		"expo-sqlite": "~16.0.10",
		"expo-status-bar": "~3.0.9",
		"expo-system-ui": "~6.0.9",
		"expo-web-browser": "~15.0.10",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-native": "0.81.5",
		"react-native-edge-to-edge": "^1.7.0",
		"react-native-gesture-handler": "~2.28.0",
		"react-native-nitro-modules": "^0.31.10",
		"react-native-reanimated": "~4.1.0",
		"react-native-safe-area-context": "~5.6.0",
		"react-native-screens": "~4.16.0",
		"react-native-unistyles": "^3.0.19",
		"react-native-web": "^0.21.0",
		"react-native-worklets": "0.5.1"
	},
	"devDependencies": {
		"ajv": "^8.17.1",
		"@babel/core": "^7.28.0",
		"@types/react": "~19.1.10",
		"babel-plugin-inline-import": "^3.0.0",
		"typescript": "~5.9.2"
	}
}
